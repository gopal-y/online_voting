{
  "name": "online_voting",
  "version": "1.0.0",
  "description": "Online Voting using blockchains backend",
  "private": true,
  "main": "server.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf build/*",
    "build": "tsc --forceConsistentCasingInFileNames",
    "serve": "npm run build && node build/server.js",
    "dev": "ts-node-dev --no-deps --ignore-watch node_modules --debug src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gopal-y/online_voting.git"
  },
  "keywords": [
    "Blockchain",
    "Voting",
    "Express",
    "Node",
    "React"
  ],
  "author": "Gopal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gopal-y/online_voting/issues"
  },
  "homepage": "https://github.com/gopal-y/online_voting#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.4",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}